{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-recommended",
    "eslint:recommended",
    "@vue/eslint-config-typescript/recommended",
    "@vue/eslint-config-prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "plugins": ["prettier"],
  "rules": {
    "vue/no-v-html": "off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}
